
package com.company;

class Matrix {
	int[][] value;

	Matrix(int[][] value) {
		this.value = value;
	}

	Matrix sum(int[][] other) {
		Matrix ret = Matrix(this.value);

		for (int i = 0; i < 2; i = i + 1) {
			for (int j = 0; j < 3; j = j + 1) {
				ret.value[i][j] = ret.value[i][j] + other[i][j];
			}
		}

		return ret;
	}

	void print() {
		for (int i = 0; i < 2; i = i + 1) {
			for (int j = 0; j < 3; j = j + 1) {
				System.out.print("${this.value[i][j]} ");
			}
			System.out.println();
		}
	}
}

class TranspileTest {
	pub void main() {
		Matrix A = Matrix([ [ 1, 2, 3 ], [ 4, 5, 6 ] ]);
		A.sum(
			[ [ 1, 2, 3 ], 
		      [ 4, 5, 6 ] ]
		).print();
	}
}