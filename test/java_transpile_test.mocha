
class TranspileTest {
	int sum(int a, int b) {
		int c = a + b;
		return c;
	}

	int factorial(int n) {
		if (n <= 0) return 1;
		else return n * factorial(n - 1);
	}

	pub void main() {
		for (int i = 0; i <= 5; i = i + 1) {
			System.out.println("${i + 1}. The factorial of ${i} is ${factorial(i)}.");
		}
	}
}