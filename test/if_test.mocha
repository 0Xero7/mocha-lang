
pub class Main {
	private float y;

	pub const int x;

	pub int sum(int a, int b) {
		return a + b;
	}

	pub int factorial(int n) {
		if (n <= 1) return 1;
		return n * factorial(n - 1);
	}

	pub int main() {
		for (i = 0; i < n; i = i + 1) {
			print(i);
			print(factorial(i));
		}

		while (i < 5) {
			factorial(i);
		}
	}
}